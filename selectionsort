#include<iostream>
using namespace std;

int main(){
	int i,j,min,n;
	
	cout<<"Enter number of elements: ";
    cin>>n;

    int arr[n];
    cout << "Enter  elements: ";
    for (i=0; i<n; i++) {
        cin>>arr[i];
    }
    
    for(i=0;i<=n-1;i++){
    	int min=i;
    	for (j=i+1;j<=n-1;j++){
    		if (arr[j]<arr[min]){
			  min=j;
			}
		}
		 int temp = arr[i];
         arr[i] = arr[min];
        arr[min] = temp;
}
	
        
     cout << "Sorted: ";
    for (int i=0; i<n; i++) {
        cout<<arr[i]<<" ";
    }
  return 0;  
}
